# Base URL for the FastAPI server in dev; Cloud Run injects this via deploy_cloud_run_web.sh.
NEXT_PUBLIC_API_BASE_URL=http://localhost:8000
NEXT_PUBLIC_ANSWER_MODE_DEFAULT=grounded
NEXT_PUBLIC_GOOGLE_AUTH_ENABLED=false
# Set only for local dev. In Cloud Run, NEXT_PUBLIC_GOOGLE_CLIENT_ID comes from --set-env-vars.
NEXT_PUBLIC_GOOGLE_CLIENT_ID=YOUR_GOOGLE_OAUTH_CLIENT_ID_HERE
